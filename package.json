{
  "name": "papyrus-index",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.2.1",
  "scripts": {
    "dev": "pnpm run parse:reset && next dev",
    "start": "npx serve out",
    "lint": "next lint",
    "prepare": "pnpm run prepare:wiki-repo && pnpm run prepare:schemas",
    "prepare:wiki-repo": "If (-not (Test-Path -Path './cache/github-wiki/')) { git clone https://github.com/BellCubeDev/papyrus-index.wiki.git ./cache/github-wiki/ } else { git -C ./cache/github-wiki/ pull }",
    "prepare:schemas": "typescript-json-schema './src/papyrus/data-structures/pure/scriptSource.ts' PapyrusScriptSourceMetadata --required --noExtraProps --strictNullChecks --skipLibCheck -o './data/SourceMetadata.schema.json' --include './src/papyrus/data-structures/pure/**' && typescript-json-schema './src/papyrus/data-structures/ignore-yaml.ts' IgnoreYaml --required --noExtraProps --strictNullChecks --skipLibCheck -o './data/IgnoreYaml.schema.json' --include './src/papyrus/data-structures/ignore-yaml.ts'",
    "download-mods": "tsx  --env-file-if-exists=./.env ./src/papyrus/scraping/download/DownloadNeededMods.ts",
    "parse:reset": "rm -f ./public/raw.json",
    "parse:raw": "tsx ./src/papyrus/parsing/parse-or-load-all.ts",
    "parse": "pnpm run parse:reset && pnpm run parse:raw",
    "guard-papyrus-logic": "$env:DO_GUARD_LOGIC='true'\npnpm run parse",
    "parse:and-index": "pnpm run parse && tsx ./src/papyrus/indexing/index-all.ts",
    "prebuild": "pnpm run download-mods && pnpm run parse",
    "build": "next build",
    "typecheck": "tsx ./typecheck.ts"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "7zip-min": "^1.4.5",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.19.0",
    "@floating-ui/react": "^0.27.3",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/eslint-plugin-next": "^15.1.6",
    "@nexusmods/nexus-api": "github:Nexus-Mods/node-nexus-api",
    "@types/7zip-min": "^1.3.3",
    "@types/hast": "^3.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "20.2.5",
    "@types/proper-lockfile": "^4.1.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/unzip-stream": "^0.3.4",
    "@vercel/og": "^0.6.5",
    "@wcj/html-to-markdown": "^2.1.1",
    "@wooorm/starry-night": "^3.6.0",
    "ajv": "^8.17.1",
    "babel-plugin-react-compiler": "19.0.0-beta-63e3235-20250105",
    "client-only": "^0.0.1",
    "eslint": "^9.19.0",
    "eslint-config-next": "^15.1.6",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-55955c9-20241229",
    "eslint-plugin-react-hooks": "^5.1.0",
    "fuzzysort": "^3.1.0",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "next": "^15.2.0",
    "next-nprogress-bar": "^2.4.3",
    "node-fetch": "^3.3.2",
    "octokit": "^4.1.0",
    "proper-lockfile": "^4.1.2",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-fontawesome": "^1.7.1",
    "react-markdown": "^9.0.3",
    "remark-breaks": "^4.0.0",
    "remove-markdown": "^0.6.0",
    "sass": "^1.83.4",
    "server-only": "^0.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "typescript-json-schema": "^0.65.1",
    "unplugin-auto-import": "^0.19.0",
    "unzip-stream": "^0.3.4",
    "vitest": "^2.1.8",
    "yaml": "^2.7.0"
  }
}
